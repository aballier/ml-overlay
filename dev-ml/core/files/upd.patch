+                   people would call GMT-5. *)
+                String.is_prefix name ~prefix:"GMT-"
+                || String.is_prefix name ~prefix:"GMT+"
+                || String.is_prefix name ~prefix:"UTC-"
+                || String.is_prefix name ~prefix:"UTC+"
