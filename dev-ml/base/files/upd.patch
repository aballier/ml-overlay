+    module Wrapped_fn3 (Input1 : T) (Input2 : T) (Input3 : T) (Output : T) = struct
+      module type S = functor (W : Wrapper) -> sig
+        val fn : Input1.t W.t -> Input2.t W.t -> Input3.t W.t -> Output.t W.t
+      end
