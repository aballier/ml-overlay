+      type ('elt, 'cmp) tree = ('elt, 'cmp) Set.Using_comparator.Tree.t
+      type ('elt, 'cmp, 'fn) create_options = ('elt, 'cmp) Comparator.Module.t -> 'fn
+      type ('elt, 'cmp, 'fn) access_options = 'fn
+    end
+
+    module Tree = struct
+      type 'elt elt = 'elt
+      type 'cmp cmp = 'cmp
